<div class="row">
  <div class="col-lg-12">
    <div class="col-lg-12">
      <div class="hpanel">
        <div class="panel-body">
          <div class="col-lg-6 text-left">
            <h2 class="font-light m-b-xs">
            Subscribers
            </h2>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<br>
<div id='setup-view-restaurant-name' class="row">
  <div class="col-lg-12">
    <div class="col-lg-12">
      <div class="hpanel">
        <div class="panel-body">
          <table id="example1" cellpadding="1" cellspacing="1" class="table table-condensed" >
            <thead>
              <tr>
                <th></th>
                <th>Email</th>
              </tr>
            </thead>
            <tbody>
              <% Subscriber.all.each do |subscriber| %>
              <tr>
                <td class="col-md-2 col-sm-2"><%= subscriber.created_at.to_date %></td>
                <td class="col-md-10 col-sm-10"><%= subscriber.email %></td>
              </tr>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>